<a href="#menu-content" class="no-click" data-toggle="collapse" data-target="#menu-content<%= number %>-<%= identifier %>">
  <h5>
    <%= title %>
  </h5>
</a>

<ul id="menu-content<%= number %>-<%= identifier %>" class="menu-content collapse <%= state %>">
  <% items.each do |item| %>
  <li>
    <%= link_to materials_path(active: 'location', material: item) do %>
    <h6>
      <%= item %>
    </h6>
    <% end %>
  </li>
  <% end %>
</ul>
