<a href="#" data-toggle="collapse" data-target="#menu-content<%= number %>">
  <h5>
    <%= title %>
  </h5>
</a>

<ul id="menu-content<%= number %>" class="menu-content collapse out">
  <% items.each do |item| %>
  <li>
    <%= link_to materials_path(active: 'location', material: item) do %>
    <h6>
      <%= item %>
    </h6>
    <% end %>
  </li>
  <% end %>
</ul>
